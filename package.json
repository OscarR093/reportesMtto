{
  "name": "reportes-mtto-backend",
  "version": "1.0.0",
  "description": "Backend modular para reportes de mantenimiento con OAuth y JWT - ES6 Modules",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No tests yet\" && exit 0",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "docker:start": "./scripts/start-dev.sh",
    "docker:stop": "./scripts/stop-dev.sh",
    "docker:fresh": "./scripts/start-dev.sh --fresh",
    "docker:clean": "./scripts/stop-dev.sh --clean",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "setup": "npm install && npm run docker:start"
  },
  "keywords": [
    "oauth",
    "jwt",
    "passport",
    "node.js",
    "express",
    "backend",
    "api",
    "react",
    "es6"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.887.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.5",
    "multer": "^2.0.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
